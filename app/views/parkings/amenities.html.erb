<div class="row">
  <div class="col-md-3">
    <%= render 'parking_menu' %>
  </div>
  <div class="col-md-9">
    <div class="panel panel-default">

      <div class="panel-heading">
        Amenities
      </div>

      <div class="panel-body">
        <div class="container">
          <%= form_for @parking do |f| %>

              <div class="row form-group">
                <div class="col-md-4">
                  <%= f.check_box :is_electric %> Electric
                </div>
                <div class="col-md-4">
                  <%= f.check_box :is_water %> Running Water
                </div>
              </div>

              <div class="row form-group">
                <div class="col-md-4">
                  <%= f.check_box :is_heating %> Heating
                </div>
              </div>
              <br/><br/>

              <div class="text-center">
                <%= f.submit "Save", class: "btn btn-form" %>
              </div>

          <% end %>
        </div>
      </div>

    </div>
  </div>
</div>
